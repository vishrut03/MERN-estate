{
   "version": 2,
   "name": "mernEstate",
   "builds": [
     { "src": "api/index.js", "use": "@vercel/node" },
     { "src": "client/package.json", "use": "@vercel/static-build" }
   ],
   "routes": [
     { "src": "/api/(.*)", "dest": "api/index.js" },
     { "src": "/(.*)", "dest": "/client/dist/$1" }
   ]
 }
 